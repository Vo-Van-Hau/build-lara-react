{
    "name": "modules/module",
    "description": "Module ModuleBuilder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "vovanhau",
            "email": "vanhau.vo@urekamedia.vn"
        }
    ],
    "require": {
        "php": "7.4.8|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Module\\": "src/"
        },
        "classmap": [
            "database/seeders"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Module\\Providers\\ModuleServiceProvider",
                "Modules\\Module\\Providers\\ModuleRouteServiceProvider"
            ],
            "aliases": {
                "Module": "Modules\\Module\\Facades\\Module"
            }
        }
    }
}

