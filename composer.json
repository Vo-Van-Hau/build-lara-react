    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^3.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",

        "modules/core": "*@dev",
        "modules/auth": "*@dev",
        "modules/users": "*@dev",
        "modules/module": "*@dev",
        "modules/publishers": "*@dev",
        "modules/products": "*@dev",
        "modules/orders": "*@dev",
        "modules/address": "*@dev",
        "modules/carts": "*@dev",
        "modules/payments": "*@dev",
        "modules/suppliers": "*@dev",
        "modules/currencies": "*@dev",
        "modules/shipping": "*@dev",
        "modules/warehouses": "*@dev",
        "modules/contacts": "*@dev",
        "modules/sellers": "*@dev",

        "frontend/core": "*@dev",
        "frontend/auth": "*@dev",
        "frontend/users": "*@dev",
        "frontend/publishers": "*@dev",
        "frontend/home": "*@dev",
        "frontend/products": "*@dev",
        "frontend/checkout": "*@dev",
        "frontend/customer": "*@dev",
        "frontend/shop": "*@dev",
        "frontend/payments": "*@dev",
        "frontend/address": "*@dev",

        "sellers/core": "*@dev",
        "sellers/auth": "*@dev",
        "sellers/users": "*@dev",
        "sellers/publishers": "*@dev",
        "sellers/dashboard": "*@dev",
        "sellers/orders": "*@dev",
        "sellers/products": "*@dev"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Frontend\\": "packages/Frontend/modules/",
            "Sellers\\": "packages/Sellers/modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
